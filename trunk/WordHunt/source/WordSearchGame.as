package  {	import flash.display.*;	import flash.events.*;		public class WordSearchGame extends Sprite {				// Constants:		// Public Properties:		// Private Properties:		private var mainMenu:MainMenuView;		private var gameView:GameView;					// Initialization:		public function WordSearchGame() {						mainMenu = new MainMenuView();			mainMenu.x = stage.stageWidth/2;			mainMenu.y = stage.stageHeight/2;			mainMenu.addEventListener('new_game',startNewGame)						gameView = new GameView()						addChild(gameView)			//addChild(mainMenu)			trace(mainMenu)		}			// Public Methods:		// Protected Methods:		private function startNewGame(e:Object=null):void{			mainMenu.visible=false;		}			}	}