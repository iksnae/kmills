package org.swxformat.events{	import flash.events.ProgressEvent;		public class SWXProgressEvent extends flash.events.ProgressEvent	{		public static const PROGRESS:String = "progress";					function SWXProgressEvent(bytesLoaded:Number, bytesTotal:Number, bubbles:Boolean = false, cancelable:Boolean = false)		{			super(PROGRESS,bubbles,cancelable);			this.bytesLoaded = bytesLoaded;			this.bytesTotal = bytesTotal;		}	}}