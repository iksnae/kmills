package org.swxformat.events{	import flash.events.Event;		public class SWXFaultEvent extends flash.events.Event	{		public static const FAULT:String = "fault";				private var data:Object;			public function get fault():Object { 			return data; 		};						function SWXFaultEvent(data:Object, bubbles:Boolean = false, cancelable:Boolean = false)		{			super(FAULT,bubbles,cancelable);			this.data = data;		}	}}